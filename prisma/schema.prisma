generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id String @id @default(uuid())
  nome String 
  email String @unique
  senha String 

  dt_criacao DateTime @default(now())
  dt_atualizacao DateTime @updatedAt()
}

